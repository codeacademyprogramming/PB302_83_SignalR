@model MemberLoginViewModel
@{
    ViewData["Title"] = "Login";
}

<section class="breadcrumb-section">
    <h2 class="sr-only">Site Breadcrumb</h2>
    <div class="container">
        <div class="breadcrumb-contents">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                    <li class="breadcrumb-item active">Login</li>
                </ol>
            </nav>
        </div>
    </div>
</section>
<!--=============================================
=            Login Register page content         =
=============================================-->
<main class="page-section inner-page-sec-padding-bottom">
    <div class="container">
        <div class="row">
            <div class="col-sm-12 col-md-12 col-xs-12 col-lg-6 mx-auto mb--30 mb-lg--0">
                <!-- Login Form s-->
                <form method="post">
                    <div class="login-form">
                        <h4 class="login-title">Customer Login</h4>
                        <div class="row">
                            <div class="col-12 mb--20">
                                <label for="email">Email</label>
                                <input class="mb-0 form-control" asp-for="Email" type="email" id="email" placeholder="Enter Your Email Address Here..">
                                <span class="text-danger" asp-validation-for="Email"></span>
                            </div>
                            <div class="col-lg-12 mb--20">
                                <label for="password">Password</label>
                                <input asp-for="Password" class="mb-0 form-control" type="password" id="password" placeholder="Enter your password">
                                <span class="text-danger" asp-validation-for="Password"></span>
                            </div>
                            <a asp-action="forgetpassword">Forget Password?</a>
                            <div class="text-danger" asp-validation-summary="ModelOnly"></div>
                            <div class="col-md-12">
                                <button type="submit" class="btn btn-outlined">Login</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</main>



